    BaseType "'s Key"
    SetFontSize 42
    SetBackgroundColor 143 146 215 255
    SetTextColor 128 0 200
    SetBorderColor 128 0 200
    Tags $soundT2
    MinimapIcon 1 Red Circle

Show
    Class "Stackable Currency"
    BaseType "Prophecy"
    SetBackgroundColor 128 0 200
    SetTextColor 192 192 192
    SetBorderColor 192 192 192
    #AutoUpdater_Prophecies_section_start
    Branch # Leagues - Prophecy - Prophecies - T0
        Tags @Prophecies_T0
        SetFontSize 45
        SetBackgroundColor 255 255 255
        SetTextColor 128 0 200
        SetBorderColor 128 0 200
        Tags $soundT0
        MinimapIcon 0 Blue Moon
        PlayEffect Blue
    Branch # Leagues - Prophecy - Prophecies - T1
        Tags @Prophecies_T1
        SetBackgroundColor 128 0 200
        SetTextColor 255 255 255
        SetBorderColor 255 255 255
        SetFontSize 43
        Tags $soundT1
        MinimapIcon 0 Brown Moon
        PlayEffect Brown
    Branch # Leagues - Prophecy - Prophecies - T2
        Tags @Prophecies_T2
        SetBackgroundColor 128 0 200 230
        SetFontSize 40
        Tags $soundT2
        MinimapIcon 1 Yellow Moon
        PlayEffect Yellow Temp
    Branch # Leagues - Prophecy - Prophecies - T3
        Tags @Prophecies_T3
        SetBackgroundColor 128 0 200 200
        SetFontSize 36
        Tags $soundT3
        MinimapIcon 2 White Moon
        PlayEffect White Temp
    Branch # Leagues - Prophecy - Prophecies - T4
        Tags @Prophecies_T4
        SetFontSize 33
        SetBackgroundColor 128 0 200 170
    #AutoUpdater_Prophecies_section_end

    Branch # Leagues - Prophecy - Prophecies - TUnknown (Missing from auto-tiering)
        SetFontSize 40
        Tags $soundTUnknown
        PlayEffect Red
        MinimapIcon 0 Red Moon

    # Setup all tiers for filterblast
    Branch # Leagues - Prophecy - Prophecies - T0
    Branch # Leagues - Prophecy - Prophecies - T1
    Branch # Leagues - Prophecy - Prophecies - T2
    Branch # Leagues - Prophecy - Prophecies - T3
    Branch # Leagues - Prophecy - Prophecies - T4

#------------------------------------
#    Perandus League
#------------------------------------
Show
    Class "Stackable Currency"
    BaseType "Perandus Coin"
    SetFontSize 37
    SetBackgroundColor 100 70 0
    SetTextColor 255 215 0
    SetBorderColor 255 215 0
    Tags $soundT3
    Branch # Leagues - Perandus - Stack of Coins (min size : 5)
        StackSize >= 5
    Branch # Leagues - Perandus - Coins
        StackSize > 0

#------------------------------------
#    Other currencies
#------------------------------------
Show
    Class "Stackable Currency"

Hide
