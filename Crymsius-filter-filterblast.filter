SetOption $Drop_Sound_Mode info [] # Remove default drop sounds. This does not affect filter sounds, only default sounds not customizable.
SetOption $Drop_Sound_Mode_0 info [default - leaves everything as it is] # no title
SetOption $Drop_Sound_Mode_1 info [all - removes drop sounds from all items] # no title
SetOption $Drop_Sound_Mode_2 info [hidden - removes drop sounds only from hidden items] # no title
SetOption $Drop_Sound_Mode_3 info [with-no-alert - removes drop sounds only from items which have no filter sound specified] # no title
SetOption $Drop_Sound_Mode_Variable dropdown [default] [all] [hidden] [with-no-alert] # no title

#all - removes drop sounds from all items affected by a filter
#hidden - removes drop sounds only from hidden items
#with-no-alert - removes drop sounds only from items which have no AlertSound specified

SetGlobalSoundPositioning $Sound_Positioning
SetGlobalDisableDropSoundMode $Drop_Sound_Mode_Variable

