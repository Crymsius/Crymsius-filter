        SetBorderColor 220 220 220

#Currency - Generic
Show
    Class "Currency"
    SetTextColor 170 158 130
    SetBorderColor 170 158 130
    SwitchTo $ShaperAnnouncements
    Branch
        Tags @Currency_Mirror
        SetFontSize 45
        SetBackgroundColor 255 255 255
        PlayAlertSound ShMirror 300
    Branch
        Tags @Currency_Exalted
        SetFontSize 45
        SetBackgroundColor 255 255 255
        PlayAlertSound ShExalted 300
    Branch
        Tags @Currency_Divine
        SetFontSize 45
        SetBackgroundColor 255 255 255
        PlayAlertSound ShDivine 300

Show
    Class "Currency"
    SetTextColor 170 158 130
    SetBorderColor 170 158 130
