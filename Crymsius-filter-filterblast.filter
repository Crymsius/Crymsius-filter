        Tags $soundTUnknown
        PlayEffect Red
        MinimapIcon 0 Red Moon

    # Setup all tiers for filterblast
    Branch # Leagues - Prophecy - Prophecies - T0
    Branch # Leagues - Prophecy - Prophecies - T1
    Branch # Leagues - Prophecy - Prophecies - T2
    Branch # Leagues - Prophecy - Prophecies - T3
    Branch # Leagues - Prophecy - Prophecies - T4

#------------------------------------
#    Perandus League
#------------------------------------
Show
    Class "Stackable Currency"
    BaseType "Perandus Coin"
    SetFontSize 37
    SetBackgroundColor 100 70 0
    SetTextColor 255 215 0
    SetBorderColor 255 215 0
    Tags $soundT3
    Branch # Leagues - Perandus - Stack of Coins (min size : 5)
        StackSize >= 5
    Branch # Leagues - Perandus - Coins
        StackSize > 0

