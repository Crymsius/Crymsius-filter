#------------------------------------
Show
    Class "Stackable Currency"
    SetFontSize 37
    SetBackgroundColor 180 195 245
    SetTextColor 10 25 90
    SetBorderColor 10 25 90
    Branch # Leagues - Essence - Remnant of Corruption
        BaseType "Remnant of Corruption"
        SetFontSize 48
        SetBackgroundColor 10 25 90
        SetTextColor 250 20 0
        SetBorderColor 250 20 0
        Tags $soundT2
        MinimapIcon 1 Red Kite

    #AutoUpdater_Essences_section_start
    Branch # Leagues - Essence - Essences - T1
        Tags @Essences_T1
        SetFontSize 50
        SetBackgroundColor 10 25 90
        SetTextColor 255 255 255
        SetBorderColor 255 255 255
        Tags $soundT1
        PlayEffect Brown
        MinimapIcon 0 Brown Kite
    Branch # Leagues - Essence - Essences - T2
        Tags @Essences_T2
        SetFontSize 40
        SetBackgroundColor 180 195 245
        SetTextColor 10 25 90
        SetBorderColor 10 25 90
        Tags $soundT2
        PlayEffect Yellow Temp
        MinimapIcon 1 Yellow Kite
    Branch # Leagues - Essence - Essences - T3
        Tags @Essences_T3
        SetFontSize 36
        Tags $soundT3
        SetBackgroundColor 180 195 245 200
        MinimapIcon 2 White Kite
    #AutoUpdater_Essences_section_end

    Branch # Leagues - Essence - Essences - TUnknown (Missing from auto-tiering)
        BaseType "Essence"
        SetBackgroundColor 10 25 90
        SetTextColor 180 195 245
        SetBorderColor 180 195 245
        SetFontSize 45
        Tags $soundTUnknown
        PlayEffect Red
        MinimapIcon 0 Red Kite

    # All tiers set up for filterblast
Show
    Class "Stackable Currency"
    BaseType "Essence"
    Branch # Leagues - Essence - Essences - T0
    Branch # Leagues - Essence - Essences - T1
    Branch # Leagues - Essence - Essences - T2
    Branch # Leagues - Essence - Essences - T3
    Branch # Leagues - Essence - Essences - T4


#------------------------------------
#    Prophecy League
#------------------------------------
Show # Leagues - Prophecy - Silver Coins
