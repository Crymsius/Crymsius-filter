        Tags @Currency_Mirror
        SetFontSize 45
        SetBackgroundColor 255 255 255
        PlayAlertSound $SoundMirror
    Branch # Currency - Generic Currency - T0 (Mirror, Exalted, Eternal,...)
        Tags @Currency_Exalted
        SetFontSize 45
        SetBackgroundColor 255 255 255
        PlayAlertSound $SoundExalted
    Branch # Currency - Generic Currency - T0 (Mirror, Exalted, Eternal,...)
        Tags @Currency_T0
        SetFontSize 45
        SetBackgroundColor 255 255 255
        PlayAlertSound $SoundT0
    Branch # Currency - Generic Currency - T1 (Divine)
        Tags @Currency_Divine
        SetFontSize 45
        SetBackgroundColor 255 255 255
        PlayAlertSound $SoundDivine
    Branch # Currency - Generic Currency - T1 (Divine)
        Tags @Currency_T1
        SetFontSize 45
        SetBackgroundColor 255 255 255
        PlayAlertSound $SoundT1
