#    Rares
#------------------------------------
#Rare ilvl 75+ special cases
Show 
    Rarity Rare
    ItemLevel >= 75
    SetFontSize 45
    SetBackgroundColor 255 255 119
    SetTextColor 100 100 0
    Branch # Rares - Highlight Corrupted - Good Bases - Weapons | Comment
        BaseType $Highlighted_Rares_Weapons
        Corrupted True
        SetFontSize 40
        SetBorderColor 250 20 0
    Branch # Rares - Highlight Corrupted - Good Bases - Armours | Comment
        BaseType $Highlighted_Rares_Armours
        Corrupted True
        SetFontSize 40
        SetBorderColor 250 20 0
    Branch # Rares - Highlight Corrupted - Good Bases - Accessories | Comment
        BaseType $Highlighted_Rares_Accessories
        Corrupted True
        SetFontSize 40
        SetBorderColor 250 20 0
    Branch # Rares - Highlight Corrupted - Other Bases | Comment
        Corrupted True
        SetFontSize 40
        SetBorderColor 250 20 0
    Branch # Rares - Highlight Identified - Good Bases - Weapons | Comment
        BaseType $Highlighted_Rares_Weapons
        Identified True
        SetFontSize 40
        SetBorderColor 100 100 0
    Branch # Rares - Highlight Identified - Good Bases - Armours | Comment
        BaseType $Highlighted_Rares_Armours
        Identified True
        SetFontSize 40
        SetBorderColor 100 100 0
    Branch # Rares - Highlight Identified - Good Bases - Accessories | Comment
        BaseType $Highlighted_Rares_Accessories
        Identified True
        SetFontSize 40
        SetBorderColor 100 100 0
    Branch # Rares - Highlight Identified - Other Bases| Comment
        Identified True
        SetFontSize 40
        SetBorderColor 100 100 0

