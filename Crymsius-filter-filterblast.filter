Show # Leagues - Prophecy - Keys
    Class "Map Fragments"
    BaseType "'s Key"
    SetFontSize 42
    SetBackgroundColor 143 146 215 255
    SetTextColor 128 0 200
    SetBorderColor 128 0 200
    Tags $soundT2
    MinimapIcon 1 Red Circle

Show
    Class "Stackable Currency"
    BaseType "Prophecy"
    SetBackgroundColor 128 0 200
    SetTextColor 192 192 192
    SetBorderColor 192 192 192
    #AutoUpdater_Prophecies_section_start
